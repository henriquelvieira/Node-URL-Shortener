{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "camelcase": [2, {"properties": "always"}],
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }] //Tratar o caso da vari√°vel Next
  }
  
}
