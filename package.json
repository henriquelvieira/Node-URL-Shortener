{
  "name": "Node-URL-Shortener",
  "version": "1.0.0",
  "description": "URL Shortener criado em Node.js e Typescript",
  "main": "./dist/app.js",
  "author": "Henrique Lopes Vieira",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:functional": "jest --projects ./src/routes --runInBand",
    "start": "node ./",
    "start:local": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/app.ts | pino-pretty -c",
    "build": "tsc -p .",
    "deploy": "tsc -p . && node ./",
    "test:start": "jest && node ./",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/app.ts | pino-pretty -c"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.7",
    "@types/pino": "^7.0.5",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-pino-logger": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.0",
    "pino": "^7.6.4",
    "shortid": "^2.2.16",
    "ts-dotenv": "^0.8.3",
    "uuid": "^8.3.2"
  }
}
